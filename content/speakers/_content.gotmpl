{{ .EnableAllLanguages }}

{{ if .Site.Params.themes.event.enableSpeakerPages }}
  {{ $context := dict "sessionizeId" .Site.Params.themes.event.sessionizeId }}
  {{ with partialCached "sessionize-api/view-all.html" $context }}
    {{ with partial "content-adapters/create-speaker-content.html" . }}
      {{ range .pages }}
        {{ $.AddPage . }}
      {{ end }}
      {{ range .resources }}
        {{ $.AddResource . }}
      {{ end }}
    {{ end }}
  {{ end }}
{{ end}}
